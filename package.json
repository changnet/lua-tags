{
	"name": "lua-tags",
	"displayName": "lua_tags",
	"description": "lua tags",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:lua"
	],
	"main": "./out/client/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "lua",
				"aliases": [
					"Lua"
				],
				"extensions": [
					".lua"
				]
			}
		],
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"@types/luaparse": "^0.2.6",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"vscode-languageclient": "^5.2.1",
		"vscode-languageserver": "^5.2.1",
		"luaparse": "^0.2.1",
		"fuzzysort": "^1.1.4"
	}
}
